    <fields>
        <fullName>Comments_vod__c</fullName>
        <description>Comments field to be used as part of the Found Sample Lot process.</description>
        <externalId>false</externalId>
        <label>Comments</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Lot_vod__c</fullName>
        <description>Field to capture the Lot # of the Found Sample Lot.</description>
        <externalId>false</externalId>
        <label>Lot #</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Mobile_ID_vod__c</fullName>
        <description>Unique external ID for future use</description>
        <externalId>false</externalId>
        <label>Mobile ID</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Quantity_vod__c</fullName>
        <description>Field to capture the Quantity of the Found Sample Lot.</description>
        <externalId>false</externalId>
        <inlineHelpText>This is an integer field. A decimal value will be truncated.</inlineHelpText>
        <label>Quantity</label>
        <precision>18</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Sample_vod__c</fullName>
        <description>Field to capture the Sample Name of the Found Sample Lot.</description>
        <externalId>false</externalId>
        <label>Sample</label>
        <picklist>
            <picklistValues>
                <fullName>Cholecap 10mg</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Cholecap 20mg</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Cholecap 30mg</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>true</sorted>
        </picklist>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <label>Sample Lot Add</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
