    <fields>
        <fullName>Allocation_Impact_Quantity_vod__c</fullName>
        <description>Allocation Impact Quantity is the formula that defines the impact that the Quantity and Confirmed Quantity have for a specific Transaction Type.  The Allocation Inventory Impact is summed for a given Sample Lot.</description>
        <externalId>false</externalId>
        <formula>IF(AND(ISPICKVAL( Type_vod__c ,&quot;BRC_vod&quot;), ISPICKVAL(Status_vod__c ,&quot;Submitted_vod&quot;),  Lot_vod__c != null ),- Quantity_vod__c,
IF(AND(ISPICKVAL( Type_vod__c ,&quot;Transfer_vod&quot;), ISPICKVAL(Status_vod__c ,&quot;Submitted_vod&quot;) ),- Quantity_vod__c,
IF(AND(ISPICKVAL(Type_vod__c ,&quot;Receipt_vod&quot;),ISPICKVAL(Status_vod__c, &quot;Submitted_vod&quot;) ), Confirmed_Quantity_vod__c,
IF(AND(ISPICKVAL(Type_vod__c ,&quot;Adjustment_vod&quot;),ISPICKVAL(Status_vod__c ,&quot;Submitted_vod&quot; )), Quantity_vod__c ,0))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Allocation Impact Quantity</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
