    <validationRules>
        <fullName>Account_Is_Person_vod</fullName>
        <active>true</active>
        <description>Ensures that the Account selected is a Person Account and not a Business Account.</description>
        <errorConditionFormula>NOT(Account_vod__r.IsPersonAccount)</errorConditionFormula>
        <errorDisplayField>Account_vod__c</errorDisplayField>
        <errorMessage>The Account must be a Person Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Collaborative_Relation_Required_Flds_vod</fullName>
        <active>true</active>
        <description>Ensures that a midlevel practitioner, a supervising physician, and a state are selected when defining Collaborative Relationship records.</description>
        <errorConditionFormula>AND(RecordType.Name = &apos;CollaborativeRelationship_vod&apos;, OR( ISBLANK(Account_vod__c), ISBLANK(Supervising_Account_vod__c),  ISPICKVAL(Collaborative_Relationship_State_vod__c, &apos;&apos;)))</errorConditionFormula>
        <errorMessage>Account, Supervising Account, and State are required when creating a Collaborative Relationship record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Collaborative_Relation_Start_End_Dt_vod</fullName>
        <active>true</active>
        <description>Ensures that when using start and end dates to define a Collaborative Relationship, the End Date of a Collaborative Relationship record is greater than its Start Date.</description>
        <errorConditionFormula>(ISBLANK(Collaborative_Relation_Start_Date_vod__c) &amp;&amp; NOT(ISBLANK(Collaborative_Relation_End_Date_vod__c))) ||
(NOT(ISBLANK(Collaborative_Relation_Start_Date_vod__c)) &amp;&amp; ISBLANK(Collaborative_Relation_End_Date_vod__c)) ||
(NOT(ISBLANK(Collaborative_Relation_Start_Date_vod__c)) &amp;&amp; NOT(ISBLANK(Collaborative_Relation_End_Date_vod__c)) &amp;&amp; (Collaborative_Relation_Start_Date_vod__c &gt;= Collaborative_Relation_End_Date_vod__c))</errorConditionFormula>
        <errorMessage>When using dates for a Collaborative Relationship record, both the Start and End Date are required, and the End Date must be greater than the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Supervising_Account_Is_Person_vod</fullName>
        <active>true</active>
        <description>Ensures that the Supervising Account selected in a Person Account and not a Business Account.</description>
        <errorConditionFormula>NOT(Supervising_Account_vod__r.IsPersonAccount)</errorConditionFormula>
        <errorDisplayField>Supervising_Account_vod__c</errorDisplayField>
        <errorMessage>The Supervising Account must be a Person Account.</errorMessage>
    </validationRules>
