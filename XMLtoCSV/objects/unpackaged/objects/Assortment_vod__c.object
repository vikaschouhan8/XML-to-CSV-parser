    <validationRules>
        <fullName>EndDate_After_StartDate_vod</fullName>
        <active>true</active>
        <description>Start date after end date</description>
        <errorConditionFormula>IF(AND(NOT(ISNULL(Start_Date_vod__c)),NOT(ISNULL(End_Date_vod__c))), Start_Date_vod__c &gt; End_Date_vod__c,false)</errorConditionFormula>
        <errorMessage>Start date after end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductBound_CannotHave_AllOthers_vod</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Product_Bound_vod__c, All_Others_vod__c)</errorConditionFormula>
        <errorMessage>A Product Bound Assortment cannot have the All Others section displayed</errorMessage>
    </validationRules>
