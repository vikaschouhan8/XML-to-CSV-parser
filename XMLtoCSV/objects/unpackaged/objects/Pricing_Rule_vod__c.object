    <validationRules>
        <fullName>Account_And_Account_Group_vod</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEN(Account_Group_vod__c)&gt;0,  LEN(Account_vod__c)&gt;0) || AND(LEN(Account_Group_vod__c)&gt;0,  LEN(Price_Book_vod__c)&gt;0) || AND(LEN(Price_Book_vod__c)&gt;0,  LEN(Account_vod__c)&gt;0)</errorConditionFormula>
        <errorMessage>Only one of Account, Account Group, or Price book should be specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Brand_Level_Rule_Type_Check</fullName>
        <active>true</active>
        <description>Only product type of &apos;Detail&apos; is allowed</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Brand_Level_Rule_vod&apos; ,NOT(ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Detail&apos;) ) )</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Only product type of &apos;Detail&apos; is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chain_And_Cross_Product_vod</fullName>
        <active>true</active>
        <description>Only one of Chain and Cross Product Rule should be checked</description>
        <errorConditionFormula>Chain_Rule_vod__c &amp;&amp; Cross_Product_Rule_vod__c</errorConditionFormula>
        <errorMessage>Only one of Chain and Cross Product Rule should be checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chain_Comparison_Product_vod</fullName>
        <active>true</active>
        <description>Comparison Product must be blank</description>
        <errorConditionFormula>Chain_Rule_vod__c == true &amp;&amp; NOT(ISBLANK( Comparison_Product_vod__c ))</errorConditionFormula>
        <errorDisplayField>Comparison_Product_vod__c</errorDisplayField>
        <errorMessage>Comparison Product must be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chain_Delivery_Order_vod</fullName>
        <active>true</active>
        <description>Chain Rule cannot be Delivery Order</description>
        <errorConditionFormula>Chain_Rule_vod__c == true &amp;&amp;  Delivery_Order_vod__c == true</errorConditionFormula>
        <errorDisplayField>Delivery_Order_vod__c</errorDisplayField>
        <errorMessage>Chain Rule cannot be Delivery Order</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chain_Limit_Bound_vod</fullName>
        <active>true</active>
        <description>Chain Rule cannot be limit bound.</description>
        <errorConditionFormula>Chain_Rule_vod__c == true &amp;&amp;  Limit_Bound_vod__c == true</errorConditionFormula>
        <errorDisplayField>Limit_Bound_vod__c</errorDisplayField>
        <errorMessage>Chain Rule cannot be limit bound.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chain_Operator_vod</fullName>
        <active>true</active>
        <description>Chain Rule Operator must be either AND or OR</description>
        <errorConditionFormula>Chain_Rule_vod__c == true &amp;&amp; ISBLANK(TEXT(Chain_Rule_Operator_vod__c))</errorConditionFormula>
        <errorDisplayField>Chain_Rule_Operator_vod__c</errorDisplayField>
        <errorMessage>Chain Rule Operator must be either AND or OR</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chain_Products_vod</fullName>
        <active>true</active>
        <description>Product must have a value and cannot equal Kit Item</description>
        <errorConditionFormula>Chain_Rule_vod__c == true &amp;&amp; (ISBLANK(Product_vod__c) ||
ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Kit Item&apos;))</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Product must have a value and cannot equal Kit Item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comp_Prod_Min_Max_Check_vod</fullName>
        <active>true</active>
        <description>Checks that Maximum Comparison Product Quantity is greater than Minimum Comparison Product Quantity</description>
        <errorConditionFormula>Product_Maximum_vod__c &lt;=  Product_Minimum_vod__c</errorConditionFormula>
        <errorDisplayField>Product_Maximum_vod__c</errorDisplayField>
        <errorMessage>The Maximum Quantity must be greater than the Minimum Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comp_Type_Check</fullName>
        <active>true</active>
        <description>Comparison Type &apos;Net Value&apos; is not supported.</description>
        <errorConditionFormula>AND(ISPICKVAL(Comparison_Type_vod__c, &apos;Net Value&apos;), 
OR(Cross_Product_Rule_vod__c =true, AND(NOT($RecordType.DeveloperName = &apos;Limit_Rule_vod&apos;), ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Order&apos;))))</errorConditionFormula>
        <errorDisplayField>Comparison_Type_vod__c</errorDisplayField>
        <errorMessage>Comparison Type &apos;Net Value&apos; is not supported.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DISCOUNT_NOT_NEG</fullName>
        <active>true</active>
        <description>Discount Override cannot be negative.</description>
        <errorConditionFormula>Discount_Override_vod__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Discount_Override_vod__c</errorDisplayField>
        <errorMessage>Discount may not be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Order_Product_Check</fullName>
        <active>true</active>
        <description>Limit Rules flagged as Delivery Order cannot be associated with a particular product. Product field must be blank for Delivery Order limit rules.</description>
        <errorConditionFormula>AND(Delivery_Order_vod__c = true, NOT(ISBLANK(Product_vod__c)))</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Delivery Order Limit Rules cannot be associated to a particular product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Order_Rule_Type_Check</fullName>
        <active>true</active>
        <description>Only pricing rules of Limit_Rule_vod record type can be flagged as Delivery Order.</description>
        <errorConditionFormula>AND(Delivery_Order_vod__c = true, NOT($RecordType.DeveloperName = &apos;Limit_Rule_vod&apos;))</errorConditionFormula>
        <errorDisplayField>Delivery_Order_vod__c</errorDisplayField>
        <errorMessage>Only Limit Rules can be flagged as Delivery Order.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Period_Date_Check_vod</fullName>
        <active>true</active>
        <description>Delivery End date must be greater than Delivery Start date</description>
        <errorConditionFormula>Delivery_End_Date_vod__c &lt;= Delivery_Start_Date_vod__c</errorConditionFormula>
        <errorDisplayField>Delivery_End_Date_vod__c</errorDisplayField>
        <errorMessage>Delivery End date must be greater than Delivery Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Period_Prod_Check_vod</fullName>
        <active>true</active>
        <description>If product type= ‘Order’, Delivery Start and End Dates must be populated</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Delivery_Period_vod&apos; &amp;&amp; ISPICKVAL(Product_vod__r.Product_Type_vod__c , &apos;Order&apos;) &amp;&amp; ( ISNULL(Delivery_Start_Date_vod__c) ||  ISNULL(Delivery_End_Date_vod__c))</errorConditionFormula>
        <errorMessage>If product type= ‘Order’, Delivery Start and End Dates must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Rule_Type_Check_vod</fullName>
        <active>true</active>
        <description>Product can be ‘empty’ or of type ‘order’</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Delivery_Period_vod&apos; &amp;&amp; Product_vod__c != null &amp;&amp; !ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Order&apos;)</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Product can be ‘empty’ or of type ‘order’</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Start_Check_vod</fullName>
        <active>true</active>
        <description>Delivery Start Date must be equal or greater than rule Start Date</description>
        <errorConditionFormula>Delivery_Start_Date_vod__c &lt; Start_Date_vod__c</errorConditionFormula>
        <errorDisplayField>Delivery_Start_Date_vod__c</errorDisplayField>
        <errorMessage>Delivery Start Date must be equal or greater than rule Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_Rule_Rule_Type_Check</fullName>
        <active>true</active>
        <description>Product can be empty or of type &apos;Order&apos; and &apos;Kit Item&apos;.</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Discount_Rule_vod&apos; ,NOT(ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Order&apos;) || ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Kit Item&apos;) || ISBLANK(  Product_vod__c ) ) )</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Product can be empty or of type &apos;Order&apos; and &apos;Kit Item&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Discount_rule_vod</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Discount_Override_vod__c ), NOT(ISBLANK( Discount_Override_Max_vod__c)),AND( NOT(ISBLANK( Discount_Override_Max_vod__c)), 
Discount_Override_Max_vod__c&lt;Discount_Override_vod__c ))</errorConditionFormula>
        <errorMessage>Discount Override Max is lower than Discount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndDate_After_StartDate_vod</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date_vod__c &gt; End_Date_vod__c</errorConditionFormula>
        <errorMessage>Start date after end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Free_Good_No_Percent_Max_Allowed</fullName>
        <active>true</active>
        <description>Throw error if Percentage Free Good Max used for regular Free Good rules</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Free_Goods_Rule_vod&apos;, NOT(ISBLANK(Free_Goods_Percentage_Max_vod__c)))</errorConditionFormula>
        <errorDisplayField>Free_Goods_Percentage_Max_vod__c</errorDisplayField>
        <errorMessage>Percentage Free Good Max cannot be populated for regular Free Good rules</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Free_Good_Percent_Comp_Type_Check</fullName>
        <active>true</active>
        <description>For Free Goods Percentage Rules, List Value Comparison Type is not supported</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Free_Goods_Percentage_Rule_vod&apos; ,ISPICKVAL(Comparison_Type_vod__c, &apos;List Value&apos;) )</errorConditionFormula>
        <errorDisplayField>Comparison_Type_vod__c</errorDisplayField>
        <errorMessage>Comparison Type &apos;List Value&apos; is not supported.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Free_Good_Percent_Max_Check</fullName>
        <active>true</active>
        <description>For Free Goods Percentage Rules, Free Goods Max is not supported</description>
        <errorConditionFormula>Free_Goods_Percentage_Max_vod__c &lt; Free_Goods_Percentage_vod__c</errorConditionFormula>
        <errorDisplayField>Free_Goods_Percentage_Max_vod__c</errorDisplayField>
        <errorMessage>Percentage Max cannot be lower than Percentage Free Good.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Free_Good_Type_Check</fullName>
        <active>true</active>
        <description>Only product type of &apos;Order&apos; is allowed</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Free_Goods_Rule_vod&apos; || $RecordType.DeveloperName = &apos;Free_Goods_Percentage_Rule_vod&apos; ,NOT(ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Order&apos;) || ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Kit Item&apos;) ) )</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Only product type of &apos;Order&apos; and &apos;Kit Item&apos; is allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Free_Goods_rule_vod</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(ISBLANK(Free_Goods_Percentage_vod__c), ISBLANK(Free_Goods_vod__c)), NOT(ISBLANK( Free_Goods_Max_vod__c )),AND( NOT(ISBLANK( Free_Goods_Max_vod__c )), NOT(ISBLANK( Free_Goods_vod__c )),
Free_Goods_Max_vod__c&lt;Free_Goods_vod__c))</errorConditionFormula>
        <errorMessage>Free Good Max is lower than Free Good. If Free Good Max is not blank, then either Free Goods or Free Good Percent must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Kit_Item_Product_Type_Check</fullName>
        <active>true</active>
        <description>Kit Item is not allowed in Cross Product Rebate.</description>
        <errorConditionFormula>AND( Cross_Product_Rule_vod__c = true, OR(  ISPICKVAL(Product_vod__r.Product_Type_vod__c , &apos;Kit Item&apos;),  ISPICKVAL( Comparison_Product_vod__r.Product_Type_vod__c,  &apos;Kit Item&apos;) ))</errorConditionFormula>
        <errorMessage>Kit Item is not allowed in Cross Product Rebate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Rule_Type_Check</fullName>
        <active>true</active>
        <description>Product can be empty or Order,Detail,Product_Group,Kit_Item</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Limit_Rule_vod&apos; ,NOT(ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Order&apos;) || ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Kit Item&apos;) || ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Detail&apos;) || ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Product Group&apos;)
|| ISBLANK(  Product_vod__c ) ) )</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Product can be empty or of type &apos;Order&apos;,&apos;Detail&apos;,&apos;Product_Group&apos;,&apos;Kit Item&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_Price_Rule_Type_Check</fullName>
        <active>true</active>
        <description>Only product type of &apos;Order&apos; and &apos;Kit Item&apos; is allowed</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;List_Price_Rule_vod&apos; ,NOT(ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Order&apos;) || ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Kit Item&apos;) ) )</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Only product type of &apos;Order&apos; and &apos;Kit Item&apos; is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Max_Greater_than_Min_vod</fullName>
