    <validationRules>
        <fullName>Product_Comparison_Check</fullName>
        <active>true</active>
        <description>Only product types Order, Detail, [empty] or Product Group is allowed.</description>
        <errorConditionFormula>NOT(ISBlank(Comparison_Product_vod__c) ||  AND(ISPICKVAL(Comparison_Product_vod__r.Product_Type_vod__c, &apos;Order&apos;) ||
 ISPICKVAL(Comparison_Product_vod__r.Product_Type_vod__c, &apos;Detail&apos;) ||
 ISPICKVAL(Comparison_Product_vod__r.Product_Type_vod__c, &apos;Product Group&apos;),  Cross_Product_Rule_vod__c = true))</errorConditionFormula>
        <errorDisplayField>Comparison_Product_vod__c</errorDisplayField>
        <errorMessage>Only product types Order, Detail, [empty], Product Group or Fixed Kit is allowed. And when the product is not empty, Cross Product Rule check box needs to be checked.</errorMessage>
