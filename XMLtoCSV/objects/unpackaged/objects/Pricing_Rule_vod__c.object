        <errorDisplayField>Quantity_Max_vod__c</errorDisplayField>
        <errorMessage>The Maximum Quantity must be greater than the Minimum Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Min_Delivery_Time_Frame_Date_Check_vod</fullName>
        <active>true</active>
