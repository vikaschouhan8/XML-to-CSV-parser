    <validationRules>
        <fullName>Product_Comparison_and_Product</fullName>
        <active>true</active>
        <description>Product must be different than Comparison Product and not empty.</description>
        <errorConditionFormula>AND(Cross_Product_Rule_vod__c = true, OR(Comparison_Product_vod__c = Product_vod__c, ISBLANK(Product_vod__c ) ))</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Product must be different than Comparison Product and not empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Group_Level_Rule_Type_Check</fullName>
        <active>true</active>
        <description>Only product type of &apos;Product Group&apos; is allowed</description>
        <errorConditionFormula>AND($RecordType.DeveloperName = &apos;Product_Group_Level_Rule_vod&apos; ,NOT(ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Product Group&apos;) ) )</errorConditionFormula>
        <errorDisplayField>Product_vod__c</errorDisplayField>
        <errorMessage>Only product type of &apos;Product Group&apos; is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Min_Max_Required</fullName>
        <active>true</active>
        <description>Cross Product Limit Rules must have Product Minimum and/or Product Maximum value</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;Limit_Rule_vod&apos; &amp;&amp;  Cross_Product_Rule_vod__c &amp;&amp; (ISNULL(Product_Minimum_vod__c) &amp;&amp; ISNULL(Product_Maximum_vod__c))</errorConditionFormula>
        <errorMessage>Product Minimum or Product Maximum is Required</errorMessage>
    </validationRules>
</CustomObject>
