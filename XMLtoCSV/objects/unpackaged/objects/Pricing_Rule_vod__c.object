        <errorConditionFormula>!ISNULL(Min_Delivery_Time_Frame_vod__c) &amp;&amp; (!ISNULL( Delivery_Start_Date_vod__c ) || !ISNULL( Delivery_End_Date_vod__c ))</errorConditionFormula>
        <errorMessage>If Min Delivery Time Frame is populated, Delivery Start Date and Delivery End Date must be empty</errorMessage>
