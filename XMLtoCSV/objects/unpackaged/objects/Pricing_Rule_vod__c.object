        <fullName>PaymentTermsRule_KitItem</fullName>
        <active>true</active>
        <description>Payment Terms Rule does not support Kit Item product type.</description>
        <errorConditionFormula>AND( $RecordType.DeveloperName = &apos;Payment_Terms_Rule_vod&apos;,  ISPICKVAL(Product_vod__r.Product_Type_vod__c, &apos;Kit Item&apos;) )</errorConditionFormula>
        <errorMessage>Payment Terms Rule does not support Kit Item product type.</errorMessage>
