    <validationRules>
        <fullName>Product_Comparison_Type_Required</fullName>
        <active>true</active>
        <description>Cross Product Limit Rules need Product Comparison Types</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;Limit_Rule_vod&apos; &amp;&amp;  Cross_Product_Rule_vod__c &amp;&amp;  ISPICKVAL(Product_Comparison_Type_vod__c,&quot;&quot;)</errorConditionFormula>
