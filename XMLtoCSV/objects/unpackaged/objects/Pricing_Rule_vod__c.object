    <validationRules>
        <fullName>Net_Price_rule_vod</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Net_Price_vod__c ), NOT(ISBLANK( Net_Price_Min_vod__c )),AND( NOT(ISBLANK( Net_Price_Min_vod__c )), 
