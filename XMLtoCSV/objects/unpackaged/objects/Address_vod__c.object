    <validationRules>
        <fullName>DEA_Puerto_Rico_vod</fullName>
        <active>true</active>
        <description>his validation rules is in place to confirm that a DEA Address for Puerto Rico has an ASSMCA Number populated.</description>
        <errorConditionFormula>AND(DEA_Address_vod__c, TRIM(TEXT( State_vod__c)) = &quot;PR&quot;, TRIM(ASSMCA_vod__c) = &quot;&quot;,  Account_vod__r.IsPersonAccount )</errorConditionFormula>
        <errorMessage>The ASSMCA Number is required when creating a DEA Address for Puerto Rico.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DEA_Schedule_Confirm_vod</fullName>
        <active>true</active>
        <description>This validation rules is in place to confirm that an Address, which has been marked as a DEA Address has a Schedule.</description>
        <errorConditionFormula>AND(DEA_Address_vod__c, OR(TRIM( DEA_Schedule_vod__c) = &quot;&quot;, TRIM(DEA_vod__c) = &quot;&quot;, TRIM(TEXT(DEA_Status_vod__c)) = &quot;&quot;, ISNULL(DEA_Expiration_Date_vod__c) ),  Account_vod__r.IsPersonAccount  )</errorConditionFormula>
        <errorMessage>The DEA Number, DEA Schedule, DEA Status, and DEA Expiration Date are required when creating a DEA Address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>License_Expiration_Date_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Not(ISBLANK( License_vod__c )),ISBLANK( License_Expiration_Date_vod__c ))</errorConditionFormula>
        <errorDisplayField>Account_vod__c</errorDisplayField>
        <errorMessage>License Expiration Date is required for State License #.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PrimaryAddressSetToInactiveNotAllowed</fullName>
        <active>true</active>
        <description>Primary Address Record Set To Inactive cannot be set.</description>
        <errorConditionFormula>Primary_vod__c &amp;&amp;  ISCHANGED( PCYC_Set_to_Inactive__c ) &amp;&amp; (PCYC_Set_to_Inactive__c == TRUE ) &amp;&amp; !ISNEW()</errorConditionFormula>
        <errorMessage>Primary Address Set To Inactive not allowed. In order to inactivate the Primary address, you must mark a different address as Primary first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Fax1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Not(ISBLANK(Fax_vod__c)),NOT(REGEX(Fax_vod__c, &quot;\\D*?(\\d\\D*?){10}&quot;)))</errorConditionFormula>
        <errorMessage>Fax 1 is in incorrect format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Fax2</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Not(ISBLANK( Fax_2_vod__c)),NOT(REGEX(Fax_2_vod__c, &quot;\\D*?(\\d\\D*?){10}&quot;)))</errorConditionFormula>
        <errorMessage>Fax 2 is in incorrect format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Zip_Code_Valid_Format</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(Zip_vod__c, &quot;\\d{5}(-\\d{4})?&quot;))</errorConditionFormula>
        <errorMessage>Zip Code Not Valid</errorMessage>
    </validationRules>
