    <fields>
        <fullName>License_Valid_To_Sample_vod__c</fullName>
        <description>License Valid to Sample formula defines if a License is Valid or Invalid and determines if an Account can receive and sign for Samples in the VBioPharma application.</description>
        <externalId>false</externalId>
        <formula>IF(LEN( TRIM(License_vod__c )) &lt;1,&quot;Invalid&quot;,IF(ISNULL( License_Expiration_Date_vod__c ) ,&quot;Invalid&quot;,IF(AND( License_Expiration_Date_vod__c &gt;= TODAY() ,ISPICKVAL(License_Status_vod__c, &quot;Valid_vod&quot;)),&quot;Valid&quot;, &quot;Invalid&quot;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>License Valid To Sample</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
