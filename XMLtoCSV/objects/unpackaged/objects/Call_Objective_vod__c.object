    <webLinks>
        <fullName>Assign_Call_Objective_To_Tactic</fullName>
        <availability>online</availability>
        <description>A button to add to the Call Objective Related list on the Account Tactic page, for linking an existing Call Objective to an existing Account Tactic</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign Call Objective To Tactic</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>var tacticId = &quot;{!Account_Tactic_vod__c.Id}&quot; ;
	var newWin=null;
	openLookupPopup(tacticId);

	function openLookupPopup(id){
		var url=&quot;apex/Assign_Call_Objective_To_Tactic_vod?tacticId=&quot;+id ;
		newWin=window.open(url, &apos;Popup&apos;,&apos;height=500,width=600,left=100,top=100,resizable=no,scrollbars=yes,toolbar=no,status=no&apos;);
        var timer = setInterval(function() {
            if(newWin.closed) {
                clearInterval(timer);
                window.location.reload();
            }
        }, 1000);
		if (window.focus){
			newWin.focus();
		}
		return false;
	}</url>
    </webLinks>
