        <fullName>Order_Status_vod__c</fullName>
        <description>Status of the Inventory Order. Values are Saved, Pending Approval, Submitted, Completed or Cancelled</description>
        <externalId>false</externalId>
        <label>Order Status</label>
        <picklist>
            <picklistValues>
                <fullName>Saved_vod</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Pending_approval_vod</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Submitted_vod</fullName>
